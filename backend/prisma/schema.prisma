datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Member {
  id        Int     @id @default(autoincrement())
  name      String
  tasks     Task[]
}

model Task {
  id        Int      @id @default(autoincrement())
  title     String
  time      DateTime
  memberId  Int
  member    Member   @relation(fields: [memberId], references: [id])
}
